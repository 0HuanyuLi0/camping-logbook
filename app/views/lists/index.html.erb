

<h2>Welcome, <%= @current_user.name %></h2>

<ul>
    <% @lists.each do |list| %>
    <li class="lists">

        <%= link_to list.title, list_path(list.id) %>
        
        <%= link_to "delete", list_path(list),method:"delete",class:"material-symbols-outlined delete_icon" %>
        
        <em>
        <p>Note: </p>
        <%= simple_format(list.note) %>
        
        </em>
        
        <br>
        

    </li>

    <% end %>
</ul>

Create New List: <%= link_to "add_box", new_list_path, class:"material-symbols-outlined" %>