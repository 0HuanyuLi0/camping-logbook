

<h2>Welcome, <%= @current_user.name %></h2>

[<%= link_to "New List", new_list_path %>]

<ul>
    <% @lists.each do |list| %>
    <li>
        <%= link_to list.title, list_path(list.id) %>
        <br>
        <%= list.note %>
        <% if @current_user.isAdmin %>
        <br>
        <%= link_to "Delete", list_path(list),method:"delete" %>
        <br><br>
        <% end %>
    </li>

    <% end %>
</ul>
