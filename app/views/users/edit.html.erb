
<h2><%= @user.email %></h2>

<%= form_with model:@user, local:true do |f| %>

    <%= f.label :name %>
    <br>
    <%= f.text_field :name %>
    <br><br>

    <%= f.label :email %>
    <br>
    <%= f.email_field :email %>
    <br><br>

    <%= f.label :isAdmin, "isAdmin" %>
    <br>
    <%= f.select :isAdmin, [["Yes",true],["No",false]] %>
    <br><br>

    <%= f.submit %>

<% end %>